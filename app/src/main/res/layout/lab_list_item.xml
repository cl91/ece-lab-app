<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:orientation="horizontal"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:padding="8dp">    <TextView        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:id="@+id/lab_list_item_name"        android:text="TEMP_TEXT"        android:layout_weight="1"    />    <TextView        android:id="@+id/lab_list_item_check"        android:layout_width="32dp"        android:layout_height="wrap_content"        android:layout_gravity="right"        android:gravity="center"        android:text=""        android:layout_marginLeft="8dp"        android:layout_marginRight="64dp"        />    <TextView        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:id="@+id/lab_list_item_auid"        android:text="TEMP_AUID"        android:gravity="right"        /></LinearLayout>